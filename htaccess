# Редирект с http на https создан автоматически при заказе бесплатного ssl-сертификата
RewriteEngine On
RewriteCond %{SERVER_PORT} !^443$
RewriteRule .* https://%{SERVER_NAME}%{REQUEST_URI} [R,L]
# Конец редиректа

Header set X-Frame-Options "ALLOW-FROM https://forum.tanki.su
Header set Content-Security-Policy "frame-ancestors 'self' https://forum.tanki.su;"